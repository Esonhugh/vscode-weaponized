{
	"name": "vscode-weaponized",
	"displayName": "Weaponized VSCode",
	"description": "using vscode to hack the world",
	"version": "0.0.1",
	"publisher": "Esonhugh",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Esonhugh/vscode-weaponized.git"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/.vscode/.zshrc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "weapon.run_command",
				"title": "weapon: Run markdown command in code block"
			},
			{
				"command": "weapon.dump_hosts",
				"title": "weapon: Dump hosts as /etc/hosts format"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.17.60",
		"@types/vscode": "^1.101.0",
		"glob": "^7.2.3",
		"mocha": "^6.2.3",
		"tslint": "^5.20.1",
		"typescript": "^3.9.10",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"ts-node": "^10.9.2",
		"yaml": "^2.8.0"
	}
}